            			<!--claim.claim:calendar view-->
			<record model="ir.ui.view" id="claim_calendar_view">
			    <field name="name">claim.claim.calendar</field>
			    <field name="model">claim.claim</field>
			    <field name="arch" type="xml">
			    	<calendar string="Reclamation" date_start="start_date" color="user_id">
			             <field name="name"/>
			             <field name="code"/>
			        </calendar>
			    </field>
			</record>
			
			
			
			
					    <!-- claim.claim: Search views -->
		    <record id="claim_request_view_search" model="ir.ui.view">
		        <field name="name">claim.claim.search</field>
		        <field name="model">claim.claim</field>
		        <field name="arch" type="xml">
		            <search string="DescriptionSearch">
		                <field name="name"/>
		                <field name="code"/>
		                <separator/>
		                <filter string="Nouvelle" name="new" domain="[('state', '=', 'new')]"/>
		                <filter string="Validee" name="done" domain="[('state', '=', 'done')]"/>
		                <filter string="Annulee" name="cancel" domain="[('state', '=', 'cancel')]"/>
		                <group  expand='0' string='Group by...'>
		                    <filter string='Responsable' domain="[]" context="{'group_by': 'user_id'}"/>
		                    <filter string='Inscription' domain="[]" context="{'group_by' : 'reg_id'}"/>
		                </group>
		            </search>
		        </field>
		    </record>
    
